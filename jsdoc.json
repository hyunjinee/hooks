{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src"],
    "includePattern": ".(jsx|js)$",
    "exclude": ["node_modules"]
  },
  "sourceType": "module",
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "readme": "README.md",
    "template": "node_modules/clean-jsdoc-theme",
    "destination": "./docs",
    "theme_opts": {
      "default_theme": "light"
    }
  }
}
