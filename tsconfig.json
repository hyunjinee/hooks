{
  "compilerOptions": {
    "jsx": "react-jsx",
    "declaration": true,
    "pretty": true,
    "strict": true,
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true,
    "downlevelIteration": true,
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler"
  },
  "exclude": ["node_modules", "dist"]
}
